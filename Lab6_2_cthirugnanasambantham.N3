<http://data.semanticweb.org/conference/eswc/2008/panel/WebScience>
      a       swc:PanelEvent ;
      rdfs:label "Does the Semantic Web Need Web Science?" ;
      swc:isSubEventOf <http://data.semanticweb.org/conference/eswc/2008> ;
      swc:moderator <http://data.semanticweb.org/person2008/wendy-hall> ;
      swc:panelist <http://data.semanticweb.org/person2008/frank-van-harmelen> , <http://data.semanticweb.org/person2008/stefan-decker> , <http://data.semanticweb.org/person2008/kieron-ohara> , <http://data.semanticweb.org/person2008/guus-schreiber> , <http://data.semanticweb.org/person2008/nigel-r-shadbolt> ;
