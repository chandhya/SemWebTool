              [ rdf:_1  <http://data.semanticweb.org/person2008/kay-uwe-schmidt> ;
                rdf:_2  <http://data.semanticweb.org/person2008/joerg-doerflinger> ;
                rdf:_3  <http://data.semanticweb.org/person2008/tirdad-rahmani> ;
                rdf:_4  <http://data.semanticweb.org/person2008/mehdi-sahbi> ;
                rdf:_5  <http://data.semanticweb.org/person2008/susan-thomas> ;
                rdf:_6  <http://data.semanticweb.org/person2008/ljiljana-stojanovic>
              ] ;
      =       <http://revyu.com/things/eswc-2008-paper-an-user-applications> , <http://semanticweb.org/id/An_User_Interface_Adaptation_Architecture_for_Rich_Internet_Applications> ;
