              [ rdf:_1  <http://data.semanticweb.org/person2009/bernhard-schandl> ;
                rdf:_2  <http://data.semanticweb.org/person2009/bernhard-haslhofer>
              ] ;
      foaf:maker <http://data.semanticweb.org/person2009/bernhard-schandl> , <http://data.semanticweb.org/person2009/bernhard-haslhofer> ;
