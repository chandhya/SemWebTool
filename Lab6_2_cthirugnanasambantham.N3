              [ rdf:_1  <http://data.semanticweb.org/person2008/stefan-dietze> ;
                rdf:_2  <http://data.semanticweb.org/person2008/alessio-gugliotta> ;
                rdf:_3  <http://data.semanticweb.org/person2008/john-domingue>
              ] ;
      =       <http://revyu.com/things/eswc-2008-paper-conceptual-situation-processes> , <http://semanticweb.org/id/Conceptual_Situation_Spaces_for_Semantic_Situation-Driven_Processes> ;
