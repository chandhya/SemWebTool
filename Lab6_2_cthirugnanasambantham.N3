              [ rdf:_1  <http://data.semanticweb.org/person2008/nicola-fanizzi> ;
                rdf:_2  <http://data.semanticweb.org/person2008/claudia-damato> ;
                rdf:_3  <http://data.semanticweb.org/person2008/floriana-esposito>
              ] ;
      =       <http://revyu.com/things/eswc-2008-paper-conceptual-clustering-detection> , <http://semanticweb.org/id/Conceptual_Clustering_and_its_Application_to_Concept_Drift_and_Novelty_Detection> ;
      foaf:maker <http://data.semanticweb.org/person2008/nicola-fanizzi> , <http://data.semanticweb.org/person2008/claudia-damato> , <http://data.semanticweb.org/person2008/floriana-esposito> .

