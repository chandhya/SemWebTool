              [ rdf:_1  <http://data.semanticweb.org/person2008/eero-hyvoenen> ;
                rdf:_2  <http://data.semanticweb.org/person2008/kim-viljanen> ;
                rdf:_3  <http://data.semanticweb.org/person2008/jouni-tuominen> ;
                rdf:_4  <http://data.semanticweb.org/person2008/katri-seppaelae>
              ] ;
      =       <http://semanticweb.org/id/Building_a_National_Semantic_Web_Ontology_and_Ontology_Service_Infrastructure----The_FinnONTO_Approach> , <http://revyu.com/things/eswc-2008-paper-building-national-approach> ;
      foaf:maker <http://data.semanticweb.org/person2008/eero-hyvoenen> , <http://data.semanticweb.org/person2008/jouni-tuominen> , <http://data.semanticweb.org/person2008/kim-viljanen> , <http://data.semanticweb.org/person2008/katri-seppaelae> .

